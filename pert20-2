#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
};

Node* head = NULL;

void insert(int val) {
    Node* newNode = new Node{val, NULL};
    if(!head) {
        head = newNode;
        newNode->next = head;
    } else {
        Node* temp = head;
        while(temp->next != head)
            temp = temp->next;
        temp->next = newNode;
        newNode->next = head;
    }
    cout << "Insert: " << val << endl;
}

void deleteNode(int val) {
    if(!head) return;
    Node *curr = head, *prev = NULL;

    do {
        if(curr->data == val) {
            if(prev) prev->next = curr->next;
            else {
                Node* temp = head;
                while(temp->next != head)
                    temp = temp->next;
                head = curr->next;
                temp->next = head;
            }
            delete curr;
            cout << "Delete: " << val << endl;
            return;
        }
        prev = curr;
        curr = curr->next;
    } while(curr != head);
}

void traverse() {
    if(!head) return;
    Node* temp = head;
    cout << "Traverse: ";
    do {
        cout << temp->data << " ";
        temp = temp->next;
    } while(temp != head);
    cout << endl;
}

int main() {
    insert(10);
    insert(20);
    insert(30);
    traverse();
    deleteNode(20);
    traverse();
}
