#include <iostream>
using namespace std;

struct Node {
    int data;
    Node* next;
};

Node *front = NULL, *rear = NULL;

void enqueue(int val) {
    Node* newNode = new Node{val, NULL};
    if(!rear) {
        front = rear = newNode;
    } else {
        rear->next = newNode;
        rear = newNode;
    }
    cout << "Enqueue: " << val << endl;
}

void dequeue() {
    if(!front) return;
    Node* temp = front;
    cout << "Dequeue: " << temp->data << endl;
    front = front->next;
    if(!front) rear = NULL;
    delete temp;
}

void display() {
    Node* temp = front;
    cout << "Queue: ";
    while(temp) {
        cout << temp->data << " ";
        temp = temp->next;
    }
    cout << endl;
}

int main() {
    enqueue(10);
    enqueue(20);
    enqueue(30);
    display();
    dequeue();
    display();
}
